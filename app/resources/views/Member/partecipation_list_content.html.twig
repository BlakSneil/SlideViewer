<table class="table table-striped table-hover table-bordered knp-table">
    <thead>
    <tr>
        <th title="{{ 'Name' }}">{{ bs_header(pagination, 'Name', 'e.name') }}</th>
        <th title="{{ 'Description' }}">{{ bs_header(pagination, 'Description', 'e.description') }}</th>
        <th title="{{ 'Date From' }}">{{ bs_header(pagination, 'Date From', 'e.dateFrom') }}</th>
        <th title="{{ 'Date To' }}">{{ bs_header(pagination, 'Date To', 'e.dateTo') }}</th>
        <th title="{{ 'Expected' }}">{{ bs_header(pagination, 'Expected', 'e.expected') }}</th>
        <th title="{{ 'Happened' }}">{{ bs_header(pagination, 'Happened', 'e.happened') }}</th>
    </tr>
    </thead>
    <tbody>
        {% for e in pagination %}
            <tr>
                <td>{{ e.event.name }}</td>
                <td>{{ e.event.description }}</td>
                <td>{{ e.event.dateFrom|date('d.m.Y') }}</td>
                <td>{{ e.event.dateTo|date('d.m.Y') }}</td>
                <td><a href=""><i class="{{ e.expected ? 'bs-checked-green' : 'bs-not-checked-red'}}"></i></a></td>
                <td>{{ e.happened }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>

<div class="knp-paginator text-right">
    {{ knp_pagination_render(pagination) }}
</div>
